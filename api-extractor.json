{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/api-extractor.schema.json",
  "compiler": {
    "configType": "tsconfig",
    "rootFolder": "."
  },
  "project": {
    "entryPointSourceFile": "./.temp/lib/index.d.ts"
  },
  "apiReviewFile": {
    "enabled": true,
    "apiReviewFolder": "./.",
    "tempFolder": "./.temp"
  },
  "apiJsonFile": {
    "enabled": false
  },
  "dtsRollup": {
    "enabled": true
  },
  "policies": {
    "namespaceSupport": "conservative"
  },
  "validationRules": {
    "missingReleaseTags": "error"
  }
}
